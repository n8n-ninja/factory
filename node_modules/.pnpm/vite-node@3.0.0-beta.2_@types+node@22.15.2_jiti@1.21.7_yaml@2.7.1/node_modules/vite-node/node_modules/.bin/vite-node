#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/manu/work/03_factory/node_modules/.pnpm/vite-node@3.0.0-beta.2_@types+node@22.15.2_jiti@1.21.7_yaml@2.7.1/node_modules/vite-node/node_modules:/Users/manu/work/03_factory/node_modules/.pnpm/vite-node@3.0.0-beta.2_@types+node@22.15.2_jiti@1.21.7_yaml@2.7.1/node_modules:/Users/manu/work/03_factory/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/manu/work/03_factory/node_modules/.pnpm/vite-node@3.0.0-beta.2_@types+node@22.15.2_jiti@1.21.7_yaml@2.7.1/node_modules/vite-node/node_modules:/Users/manu/work/03_factory/node_modules/.pnpm/vite-node@3.0.0-beta.2_@types+node@22.15.2_jiti@1.21.7_yaml@2.7.1/node_modules:/Users/manu/work/03_factory/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vite-node.mjs" "$@"
else
  exec node  "$basedir/../../vite-node.mjs" "$@"
fi
