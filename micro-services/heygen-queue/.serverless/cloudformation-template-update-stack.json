          "Fn::Join": [
            "-",
            [
              "heygen-queue",
              "dev",
              {
                "Ref": "AWS::Region"
              },
              "lambdaRole"
            ]
          ]
        }
      }
    },
    "HelloWorldLambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": {
            "Ref": "ServerlessDeploymentBucket"
          },
          "S3Key": "serverless/heygen-queue/dev/1747898005024-2025-05-22T07:13:25.024Z/helloWorld.zip"
